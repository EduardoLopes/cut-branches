{
	"name": "cut-branches",
	"version": "0.3.1",
	"scripts": {
		"dev:svelte": "vite dev",
		"build:svelte": "vite build",
		"dev": "tauri dev",
		"build": "tauri build",
		"build:debug": "RUST_BACKTRACE=1 tauri build --debug",
		"preview": "vite preview",
		"lint": "eslint . --max-warnings 0",
		"lint:fix": "eslint . --fix",
		"format:fix": "prettier --write .",
		"format": "prettier --check .",
		"check": "svelte-check",
		"tauri": "tauri",
		"update-all": "pnpm update -irL && cd src-tauri && cargo update && cd ..",
		"changeset": "changeset",
		"prepare": "svelte-kit sync && panda codegen && lefthook install",
		"version-packages": "git push && changeset version",
		"test": "vitest --run --coverage --silent",
		"test:watch": "vitest --watch --coverage",
		"test:ui": "vitest --coverage --ui",
		"release": "pnpm update-tauri-version && changeset tag",
		"clean": "rm -rf dist && rm -rf node_modules",
		"update-tauri-version": "node scripts/update-tauri-version.mjs",
		"link-pindoba": "bash scripts/manage-pindoba-links.sh link",
		"unlink-pindoba": "bash scripts/manage-pindoba-links.sh unlink"
	},
	"devDependencies": {
