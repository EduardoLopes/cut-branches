{
	"name": "cut-branches",
	"version": "0.5.0",
	"scripts": {
		"dev:svelte": "vite dev",
		"build:svelte": "vite build",
		"dev": "tauri dev",
		"build": "tauri build",
		"build:debug": "RUST_BACKTRACE=1 tauri build --debug",
		"preview": "vite preview",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format:fix": "prettier --write .",
		"format": "prettier --check .",
		"check": "svelte-check",
		"tauri": "tauri",
		"update-all": "pnpm update -irL && cd src-tauri && cargo update && cd ..",
		"changeset": "changeset",
		"prepare": "svelte-kit sync && panda codegen && lefthook install",
		"version-packages": "git push && changeset version && pnpm update-tauri-version",
		"test": "vitest --run --coverage --silent",
		"test:watch": "vitest --watch --coverage",
		"test:ui": "vitest --coverage --ui",
		"test:cargo": "cargo test --manifest-path src-tauri/Cargo.toml",
		"test:cargo:coverage": "cargo llvm-cov --manifest-path src-tauri/Cargo.toml",
		"test:cargo:coverage:html": "cargo llvm-cov --manifest-path src-tauri/Cargo.toml --html",
		"test:cargo:coverage:open": "cargo llvm-cov --manifest-path src-tauri/Cargo.toml --html && open src-tauri/target/llvm-cov/html/index.html",
		"test:cargo:coverage:text": "cargo llvm-cov --manifest-path src-tauri/Cargo.toml --text",
		"test:cargo:coverage:lcov": "cargo llvm-cov --manifest-path src-tauri/Cargo.toml --lcov --output-path src-tauri/lcov.info",
		"release": "changeset tag",
		"clean": "rm -rf dist && rm -rf node_modules",
		"update-tauri-version": "node scripts/update-tauri-version.mjs",
		"link-pindoba": "bash scripts/manage-pindoba-links.sh link",
		"unlink-pindoba": "bash scripts/manage-pindoba-links.sh unlink",
		"rust:fmt": "cd src-tauri && cargo fmt --all -- --check",
		"rust:fmt:fix": "cd src-tauri && cargo fmt --all",
		"rust:clippy": "cd src-tauri && cargo clippy -- -D warnings",
		"rust:test": "cd src-tauri && cargo test --all-features",
		"rust:check": "pnpm rust:fmt && pnpm rust:clippy && pnpm rust:test"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.7",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/js": "^9.35.0",
		"@iconify/svelte": "^5.0.1",
		"@pandacss/dev": "^1.3.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.38.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tanstack/svelte-query-devtools": "^5.87.4",
		"@tauri-apps/cli": "^2.8.4",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/ramda": "^0.31.1",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"@vitest/coverage-istanbul": "3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"csstype": "^3.1.3",
		"dotenv-cli": "^10.0.0",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-svelte": "^3.12.3",
		"eslint-plugin-unused-imports": "^4.2.0",
		"globals": "^16.4.0",
		"i": "^0.3.7",
		"jsdom": "^26.1.0",
		"lefthook": "^1.13.0",
		"prettier": "~3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "5.38.10",
		"svelte-check": "^4.3.1",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0",
		"vite": "^7.1.5",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^5.2.6",
		"@pindoba/panda": "alpha",
		"@pindoba/panda-preset": "alpha",
		"@pindoba/styles": "alpha",
		"@pindoba/svelte-alert": "alpha",
		"@pindoba/svelte-badge": "alpha",
		"@pindoba/svelte-button": "alpha",
		"@pindoba/svelte-checkbox": "alpha",
		"@pindoba/svelte-dialog": "alpha",
		"@pindoba/svelte-group": "alpha",
		"@pindoba/svelte-loading": "alpha",
		"@pindoba/svelte-navigation": "alpha",
		"@pindoba/svelte-pagination": "alpha",
		"@pindoba/svelte-popover": "alpha",
		"@pindoba/svelte-radio": "alpha",
		"@pindoba/svelte-select": "alpha",
		"@pindoba/svelte-text-input": "alpha",
		"@pindoba/svelte-theme-mode-select": "alpha",
		"@radix-ui/colors": "^3.0.0",
		"@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@8c9ce998d84573adafedcbac4488159cd75e2473",
		"@tauri-apps/api": "^2.8.0",
		"@tauri-apps/plugin-dialog": "~2.4.0",
		"@tauri-apps/plugin-shell": "~2.3.1",
		"@types/lodash": "^4.17.20",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"hash-it": "^6.0.0",
		"just-debounce-it": "^3.2.0",
		"lodash": "^4.17.21",
		"ramda": "^0.31.3",
		"svelte-exmarkdown": "^5.0.2",
		"zod": "^4.1.8"
	},
	"pnpm": {
		"patchedDependencies": {
			"@changesets/changelog-github@0.5.1": "patches/@changesets__changelog-github@0.5.1.patch"
		}
	},
	"packageManager": "pnpm@10.6.4+sha512.da3d715bfd22a9a105e6e8088cfc7826699332ded60c423b14ec613a185f1602206702ff0fe4c438cb15c979081ce4cb02568e364b15174503a63c7a8e2a5f6c"
}
