{
	"name": "cut-branches",
	"version": "0.0.1",
	"scripts": {
		"dev:svelte": "svelte-kit dev",
		"dev": "concurrently \"npm run dev:svelte\" \"wait-on http://localhost:3000 && tauri dev\"",
		"build": "svelte-kit build && tauri build",
		"build:debug": "svelte-kit build && tauri build --debug",
		"preview": "svelte-kit preview",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"tauri": "tauri"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.10",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@zerodevx/svelte-toast": "^0.7.1",
		"carbon-icons-svelte": "^10.45.1",
		"concurrently": "^7.0.0",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"prettier": "~2.6.0",
		"prettier-plugin-svelte": "^2.6.0",
		"sass": "^1.49.9",
		"svelte": "^3.46.4",
		"svelte-preprocess": "^4.10.4",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2",
		"wait-on": "^6.0.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.4",
		"@lukeed/uuid": "^2.0.0",
		"@tauri-apps/api": "^1.0.0-rc.2",
		"@tauri-apps/cli": "^1.0.0-rc.7",
		"cookie": "^0.4.2"
	}
}
