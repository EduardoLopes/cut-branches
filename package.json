{
  "name": "cut-branches",
  "version": "0.0.1",
  "scripts": {
    "dev:svelte": "svelte-kit dev",
    "dev": "concurrently \"npm run dev:svelte\" \"wait-on http://localhost:3000 && tauri dev\"",
    "build": "svelte-kit build && tauri build",
    "build:debug": "svelte-kit build && tauri build --debug",
    "preview": "svelte-kit preview",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "tauri": "tauri"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.10",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.3.1",
    "prettier-plugin-svelte": "^2.3.0",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.4.5",
    "@lukeed/uuid": "^2.0.0",
    "@tauri-apps/api": "*",
    "@tauri-apps/cli": "*",
    "cookie": "^0.4.1"
  }
}
